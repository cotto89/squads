{
  "name": "squads",
  "version": "0.0.1",
  "description": "Application state transaction manager",
  "scripts": {
    "prebuild:dist": "npm run clean:dist",
    "build:dist": "NODE_ENV=production babel src -d dist",
    "clean:dist": "rimraf ./dist",
    "dev:example:counter": "webpack-dev-server --config ./webpack/counter.conf.js",
    "prebuild:example:counter": "npm run clean:example:counter",
    "build:example:counter": "NODE_ENV=production webpack -p --progress --config ./webpack/counter.conf.js",
    "clean:example:counter": "rimraf ./docs/example/counter/bundle.*",
    "test": "NODE_ENV=test mocha --recursive",
    "server": "lite-server",
    "build": "npm run build:dist && npm run build:example:counter",
    "clean": "npm run clean:dist && npm run clean:example:counter"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-unassert": "^2.1.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-jsdoc-to-assert": "^4.0.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.2.0",
    "karma-notify-reporter": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "lite-server": "^2.2.2",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "phantomjs-prebuilt": "^2.1.12",
    "postcss-loader": "^0.13.0",
    "power-assert": "^1.4.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-merge": "^0.14.1",
    "webpack-notifier": "^1.4.1"
  },
  "dependencies": {
    "lodash.isfunction": "^3.0.8",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isstring": "^4.0.1",
    "lodash.merge": "^4.6.0",
    "lodash.topairs": "^4.3.0"
  }
}
